@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
    ViewBag.Title = "Stocks Report";
    Session["MenuKey"] = 31;
}
<div class="container-fluid">
    <header>
        <h3 class="page-title">
            @ViewBag.Title
        </h3>        
    </header>
    <form class="form-horizontal">
        <div class="row-fluid">
            <div class="span12">
                <div class="portlet box" id="reportArea">
                    <iframe id="StocksReport" style="min-height: 600px" src="~/ReportsFiles/Stocks.aspx" width="80%"></iframe>
                </div>
            </div>
        </div>
    </form>
</div>
@Html.Partial("_ReportSpinner")

